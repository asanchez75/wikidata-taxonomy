{
  "name": "wikidata-taxonomy",
  "version": "0.6.4",
  "description": "command line tool to extract taxonomies from Wikidata",
  "author": "Jakob VoÃŸ",
  "license": "MIT",
  "main": "index.js",
  "browser": "index.js",
  "bin": {
    "wdtaxonomy": "./wdtaxonomy.js",
    "wdmappings": "./wdmappings.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichtich/wikidata-taxonomy.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/standard *.js lib/*.js",
    "fix": "./node_modules/.bin/standard --fix *.js lib/*.js",
    "build": "./node_modules/.bin/browserify . -i wikidata-sdk -i chalk -i request-promise -s wdt -o dist/wikidata-taxonomy.js",
    "test": "./node_modules/.bin/mocha --reporter spec --no-colors"
  },
  "bugs": {
    "url": "https://github.com/nichtich/wikidata-taxonomy/issues"
  },
  "keywords": [
    "wikidata",
    "taxonomy",
    "classification",
    "hierarchy",
    "cli"
  ],
  "homepage": "https://github.com/nichtich/wikidata-taxonomy#readme",
  "dependencies": {
    "beacon-links": "^0.1.0",
    "chalk": "^1.1.3",
    "commander": "^2.10.0",
    "request": "^2.3.4",
    "request-promise-native": "^1.0.0",
    "wikidata-sdk": "^3.2.4"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.0",
    "browserify": "^14.4.0",
    "standard": "^10.0"
  },
  "conf.py": {
    "project": "wdtaxonomy",
    "latex_elements": {
      "papersize": "a4paper"
    }
  }
}
